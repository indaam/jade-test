mixin searcform
  .wrap-search
    // Defauult Search 
    form#searchform(action='#/', method='get', role='search')
      div
        label.screen-reader-text(for='s') Search for:
        input#s(type='text', placeholder='Search for products', name='s', value='')
        input#searchsubmit(type='submit', value='Search')
        input(type='hidden', value='product', name='post_type')

    // Serch If use yith ajaxsearchform
    .yith-ajaxsearchform-container
      form#yith-ajaxsearchform(role='search', method='get', action='http://tokokoo.com/demo/shirtbox/')
        div
          label.assistive-text(for='s') Search
          input#yith-s.field.txt(type='search', value='', name='s', placeholder='Search for products', autocomplete='off', style='background-color: transparent; background-position: initial initial; background-repeat: initial initial;')
          input(type='hidden', name='post_type', value='product')
      .autocomplete-suggestions(style='position: absolute; max-height: 300px; z-index: 9999; width: 145px;')
        .autocomplete-suggestion(data-index='0')
          strong Woo
          | Album #1
        .autocomplete-suggestion(data-index='1')
          strong Woo
          | Album #2
        .autocomplete-suggestion(data-index='2')
          strong Woo
          | Album #3
    // End .wrap-search

